{
  "name": "ts-next-boilerplate",
  "version": "1.0.0",
  "description": "TypeScript Next Boilerplate",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"**/*.{ts,tsx}\" --ignore-pattern node_modules/",
    "test-clear": "rm -rf test/tests/ui/__snapshots__;rm -rf coverage;rm -rf .next",
    "test-coverage": "jest --coverage",
    "test-coverage-dev": "NODE_ENV=development jest --coverage --watchAll",
    "test": "jest",
    "test-dev": "NODE_ENV=development npx jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinPar/ts-next-boilerplate.git"
  },
  "author": "Ignacio <nacho@binpar.com> (https://www.binpar.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BinPar/ts-next-boilerplate/issues"
  },
  "homepage": "https://github.com/BinPar/ts-next-boilerplate#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "next": "^9.4.4",
    "react": "^16.9",
    "react-dom": "^16.9"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "ts-jest": "^26.1.2",
    "typescript": "^3.6.3"
  }
